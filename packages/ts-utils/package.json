{
  "name": "@belgattitude/ts-utils",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/ts-utils"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js --runInBand",
    "test-unit-watch": "vitest watch --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@size-limit/file": "7.0.8",
    "@types/jest": "28.1.2",
    "@types/node": "18.0.0",
    "@vitest/ui": "0.15.1",
    "c8": "7.11.3",
    "cross-env": "7.0.3",
    "es-check": "6.2.1",
    "eslint": "8.17.0",
    "get-tsconfig": "patch:get-tsconfig@npm%3A4.0.3#~/.yarn/patches/get-tsconfig-npm-4.0.3-3ca65a6ddd.patch",
    "jest": "28.1.1",
    "rimraf": "3.0.2",
    "size-limit": "7.0.8",
    "ts-jest": "28.0.5",
    "tslib": "2.4.0",
    "tsup": "6.1.2",
    "type-fest": "2.13.1",
    "typescript": "4.7.3",
    "vite": "2.9.12",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.15.1"
  },
  "engines": {
    "node": ">=14.16"
  }
}
