{
  "name": "@belgattitude/db-main",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/db-main"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "main": "./src/index.ts",
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "scripts": {
    "prisma-migrate": "dotenv-flow -- yarn prisma migrate",
    "prisma-migrate-reset": "dotenv-flow -- yarn prisma migrate reset",
    "prisma-db-seed": "dotenv-flow -- yarn prisma db seed",
    "prisma-db-push": "dotenv-flow -- yarn prisma db push",
    "prisma-studio": "dotenv-flow -- yarn prisma studio",
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "prettier-check": "yarn run --top-level prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/db-main.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "run-s test-unit",
    "test-unit": "echo \"No unit tests yet\"",
    "test-e2e": "jest --config e2e/jest.config.js --maxWorkers=50% --color $@",
    "debug-test-e2e": "cross-env DEBUG=testcontainers jest --config e2e/jest.config.js --maxWorkers=50% --color $@",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@pothos/core": "3.12.1",
    "@pothos/plugin-prisma": "3.13.1",
    "@prisma/client": "4.0.0",
    "@soluble/dsn-parser": "1.4.1",
    "@types/jest": "28.1.4",
    "@types/node": "18.0.1",
    "camelcase": "7.0.0",
    "cross-env": "7.0.3",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "eslint": "8.19.0",
    "execa": "5.1.1",
    "graphql": "16.5.0",
    "is-port-reachable": "3.1.0",
    "jest": "28.1.2",
    "keyword-extractor": "0.0.23",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "prisma": "4.0.0",
    "rimraf": "3.0.2",
    "transliteration": "2.3.5",
    "ts-jest": "28.0.5",
    "tsx": "3.7.1",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "@pothos/plugin-prisma": "^3.6.1",
    "@prisma/client": "^3.13.0 || ^4.0.0"
  }
}
