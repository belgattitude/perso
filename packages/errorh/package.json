{
  "name": "@belgattitude/errorh",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/errorh"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "build-doc": "rimraf --no-glob ./docs/api && typedoc --githubPages false --plugin typedoc-plugin-markdown",
    "prettier-check": "yarn run --top-level prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/errorh.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js --runInBand",
    "test-unit-watch": "vitest watch --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@size-limit/file": "8.0.1",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.13",
    "@vitest/coverage-istanbul": "0.22.1",
    "@vitest/ui": "0.22.1",
    "cross-env": "7.0.3",
    "es-check": "7.0.0",
    "eslint": "8.23.0",
    "get-tsconfig": "4.2.0",
    "jest": "29.0.1",
    "rimraf": "3.0.2",
    "size-limit": "8.0.1",
    "ts-jest": "28.0.8",
    "tslib": "2.4.0",
    "tsup": "6.2.3",
    "type-fest": "2.19.0",
    "typedoc": "0.23.11",
    "typedoc-plugin-markdown": "3.13.4",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.22.1"
  },
  "engines": {
    "node": ">=14.16"
  }
}
