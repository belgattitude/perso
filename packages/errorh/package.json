{
  "name": "@belgattitude/errorh",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/errorh"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/cjs/index.js",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.js",
      "module": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf --no-glob ./dist && rollup -c rollup.config.js",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "build-doc": "rimraf --no-glob ./docs/api && typedoc --plugin typedoc-plugin-markdown",
    "prettier-check": "yarn run --top-level prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/errorh.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-watch": "vitest --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@size-limit/esbuild": "8.1.0",
    "@size-limit/file": "8.1.0",
    "@size-limit/webpack": "8.1.0",
    "@types/node": "18.7.16",
    "@vitest/coverage-istanbul": "0.23.2",
    "@vitest/ui": "0.23.2",
    "cross-env": "7.0.3",
    "es-check": "7.0.1",
    "esbuild": "0.15.7",
    "eslint": "8.23.0",
    "get-tsconfig": "4.2.0",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-esbuild": "4.10.1",
    "size-limit": "8.1.0",
    "typedoc": "0.23.14",
    "typedoc-plugin-markdown": "3.13.6",
    "typescript": "4.8.3",
    "vite": "3.1.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.2"
  },
  "engines": {
    "node": ">=14.16"
  }
}
