{
  "name": "@belgattitude/errorh",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/errorh"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.js",
      "module": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "build-doc": "rimraf --no-glob ./docs/api && typedoc --githubPages false --plugin typedoc-plugin-markdown",
    "prettier-check": "pnpm run -w prettier --check --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "pnpm run -w prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/errorh.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js --runInBand",
    "test-unit-watch": "vitest watch --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@rollup/plugin-commonjs": "22.0.2",
    "@size-limit/file": "8.0.1",
    "@size-limit/webpack": "8.0.1",
    "@size-limit/webpack-why": "8.0.1",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.15",
    "@vitest/coverage-istanbul": "0.23.1",
    "@vitest/ui": "0.23.1",
    "cross-env": "7.0.3",
    "es-check": "7.0.0",
    "esbuild": "0.15.7",
    "eslint": "8.23.0",
    "get-tsconfig": "4.2.0",
    "jest": "29.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.79.0",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-esbuild": "4.10.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript": "1.0.1",
    "size-limit": "8.0.1",
    "terser": "5.15.0",
    "ts-jest": "28.0.8",
    "typedoc": "0.23.14",
    "typedoc-plugin-markdown": "3.13.6",
    "typescript": "4.8.2",
    "vite": "3.1.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.23.1"
  },
  "engines": {
    "node": ">=14.16"
  }
}
