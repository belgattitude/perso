{
  "name": "@belgattitude/base-ui",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/perso",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/perso",
    "directory": "packages/base-ui"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf --no-glob ./dist ./coverage ./build ./tsconfig.tsbuildinfo",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "test": "yarn test-unit",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js --runInBand",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "prettier-check": "yarn run --top-level prettier --check --cache --cache-location ../../.cache/prettier/base-ui.prettiercache --cache-strategy metadata --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/base-ui.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --output-dir build/storybook",
    "serve-storybook": "sirv build/storybook --cors --port 8888"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@mui/joy": "5.0.0-alpha.56"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@belgattitude/eslint-config-bases": "workspace:^",
    "@emotion/babel-plugin": "11.10.5",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mdx-js/react": "2.1.5",
    "@size-limit/file": "8.1.0",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-docs": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-interactions": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/mdx2-csf": "0.0.3",
    "@storybook/react": "6.5.14",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/dom": "8.19.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.9",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.2.0",
    "@vitest/coverage-istanbul": "0.25.6",
    "@vitest/ui": "0.25.6",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.0",
    "cross-env": "7.0.3",
    "es-check": "7.0.1",
    "eslint": "8.29.0",
    "get-tsconfig": "4.2.0",
    "happy-dom": "7.7.2",
    "jest": "29.3.1",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "29.3.1",
    "microbundle": "0.15.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "7.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "require-from-string": "2.0.2",
    "rimraf": "3.0.2",
    "sirv": "2.0.2",
    "sirv-cli": "2.0.2",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "tsup": "6.5.0",
    "typescript": "4.9.3",
    "vite": "3.2.4",
    "vite-plugin-svgr": "2.2.2",
    "vite-tsconfig-paths": "3.6.0",
    "vitest": "0.25.6",
    "webpack": "5.75.0"
  },
  "engines": {
    "node": ">=14.16"
  }
}
