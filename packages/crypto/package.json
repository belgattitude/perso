    "rimraf": "3.0.2",
    "size-limit": "7.0.8",
    "ts-jest": "28.0.5",
    "tslib": "2.4.0",
    "tsup": "6.1.3",
    "typescript": "4.7.4",
    "vite": "2.9.13",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.17.0"
  },
  "engines": {
    "node": ">=14.16"
  }
}
