  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo",
    "dev": "next",
    "build": "next build",
    "build-sitemap": "next-sitemap --config ./next-sitemap.config.js",
    "build-graphql-codegen": "graphql-codegen",
    "build-fast": "cross-env NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_DISABLE_SENTRY=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 next build",
    "vercel-build": "next build && yarn build-sitemap",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true NEXTJS_IGNORE_TYPECHECK=1 NEXTJS_IGNORE_ESLINT=1 NEXTJS_SENTRY_UPLOAD_DRY_RUN=1 yarn build",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "test": "yarn test-unit",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js --runInBand",
    "test-unit-watch": "vitest watch --ui",
    "test-e2e-local": "cross-env E2E_WEBSERVER_MODE=BUILD_AND_START playwright test",
    "test-e2e": "playwright test",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "prettier-check": "yarn run --top-level prettier --check --cache --cache-location ../../.cache/prettier/website.prettiercache --cache-strategy metadata --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "prettier-fix": "yarn run --top-level prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore \"./**/*.{js,jsx,cjs,mjs,ts,tsx,mts,md,mdx,json,css,scss,less}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/website.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
